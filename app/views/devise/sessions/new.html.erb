<h3>管理员登录</h3>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :邮箱 %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :密码 %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="actions">
    <%= f.submit "登录" %>
  </div>
<% end %>
